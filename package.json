{
  "name": "example-nestjs-graphql-prisma",
  "version": "0.2.0",
  "private": true,
  "description": "An example NestJS + GraphQL + Prisma",
  "license": "MIT",
  "author": "nazna <naznagg@gmail.com>",
  "scripts": {
    "predev": "npm run typegen",
    "dev": "nest start --watch",
    "start": "nest start",
    "prebuild": "rimraf dist && npm run typegen",
    "build": "nest build",
    "typegen": "ts-node src/typegen.ts",
    "db": "rimraf ./prisma/dev.db && npm run db:generate && npm run db:push && npm run db:seed",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed --preview-feature",
    "db:studio": "prisma studio",
    "lint": "npm run eslint && npm run prettier",
    "eslint": "eslint --fix --ext .ts .",
    "prettier": "prettier --write ."
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "root": true
  },
  "dependencies": {
    "@nestjs/common": "8.0.4",
    "@nestjs/core": "8.0.4",
    "@nestjs/graphql": "8.0.2",
    "@nestjs/platform-express": "8.0.4",
    "@prisma/client": "2.27.0",
    "apollo-server-express": "2.25.2",
    "class-validator": "0.13.1",
    "graphql": "15.5.1",
    "nanoid": "3.1.23",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@nestjs/cli": "8.0.2",
    "@types/express": "4.17.13",
    "@types/node": "16.4.0",
    "@typescript-eslint/eslint-plugin": "4.28.4",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "prettier": "2.3.2",
    "prettier-plugin-packagejson": "2.2.11",
    "prisma": "2.27.0",
    "rimraf": "3.0.2",
    "ts-morph": "11.0.3",
    "ts-node": "10.1.0",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": "14.x",
    "npm": "7.x"
  },
  "volta": {
    "node": "14.17.3",
    "npm": "7.20.0"
  }
}
