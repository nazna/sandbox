{
  "name": "example-graphql-versioning",
  "version": "2.0.0",
  "private": true,
  "description": "An example to learn GraphQL versioning",
  "license": "MIT",
  "author": "nazna <naznagg@gmail.com> (https://github.com/nazna)",
  "scripts": {
    "dev": "nest start --watch",
    "start": "nest start",
    "prebuild": "rimraf dist && npm run typegen",
    "build": "nest build",
    "lint": "npm run typecheck && npm run prettier",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write .",
    "typegen": "node --require esbuild-register src/v2/generate-typings.ts"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@nestjs/common": "7.6.15",
    "@nestjs/core": "7.6.15",
    "@nestjs/graphql": "7.10.3",
    "@nestjs/platform-express": "7.6.15",
    "apollo-server-express": "2.23.0",
    "graphql": "15.5.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.7"
  },
  "devDependencies": {
    "@nestjs/cli": "7.6.0",
    "@types/express": "4.17.11",
    "@types/node": "14.14.41",
    "esbuild-register": "2.5.0",
    "prettier": "2.2.1",
    "prettier-plugin-organize-imports": "1.1.1",
    "prettier-plugin-packagejson": "2.2.10",
    "rimraf": "3.0.2",
    "typescript": "4.2.4"
  },
  "engines": {
    "node": "14.x",
    "npm": "7.x"
  },
  "volta": {
    "node": "14.16.1",
    "npm": "7.10.0"
  }
}
