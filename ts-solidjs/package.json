{
  "name": "example-solidjs",
  "version": "0.0.1",
  "private": true,
  "description": "Example frontend application with SolidJS",
  "license": "MIT",
  "author": "nazna <naznagg@gmail.com> (https://nazna.dev)",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "lint": "npm run typecheck && npm run eslint && npm run prettier",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint --ignore-path .gitignore --fix .",
    "prettier": "prettier --ignore-path .gitignore --write ."
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "solid-app-router": "0.2.1",
    "solid-js": "1.3.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "c8": "7.11.0",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "prettier": "2.5.1",
    "typescript": "4.5.5",
    "vite": "2.7.13",
    "vite-plugin-solid": "2.2.5",
    "vite-plugin-windicss": "1.6.3",
    "vitest": "0.2.7"
  },
  "engines": {
    "node": "17.x",
    "npm": "8.x"
  }
}
